{
    "name": "Bookmarks Organizer Proto",
    "version": "0.7.0",
    "manifest_version": 2,
    "author": "Jaseem V V",
    "description": "Organize Bookmarks",
    "permissions": ["bookmarks"],
    "background": {
        "scripts": ["events.js"],
        "persistent": false
      },
    "browser_action": {
        "default_title": "Bookmarks Organizer Proto (by kadaj)",
        "default_icon": "res/icons/bo-16.png",
        "default_popup": "bookmarks-organizer.html"
    },
    "icons": {
        "16": "res/icons/bo-16.png",
        "48": "res/icons/bo-48.png",
        "128": "res/icons/bo-128.png"
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
